{
  "api": {
    "endpoint": "http://localhost:3030/pushTestData",
    "headers": {
      "Content-type": "application/json",
      "Accept": "*/*"
    },
    "auth": {
      "user": "user",
      "pass": "pass"
    },
    "payload": {
      "key1": "key1",
      "key2": "key2",
      "key3": {
        "key3_sub": "key3_sub"
      },
      "data": "${data_files}"
    },
    "request_method": "put"
  },
  "data_files": {
      "files": ["./dataSP.json","./dataSP2.json"],
      "csv_delimiter": ","
  },
  "schedule":{
    "limitation": 5,
    "duration": 2
  }
}